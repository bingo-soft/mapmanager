{
  "name": "@bingo_soft/mapmanager",
  "version": "1.93.1",
  "description": "Map manager for GIS projects",
  "main": "src/MapManager.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/bingo-soft/mapmanager.git"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testRegex": "./test/.*.ts$",
    "rootDir": ".",
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "<rootDir>/config/CSSStub.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(ol)/)"
    ]
  },
  "scripts": {
    "serve": "parcel serve src/MapManager.ts --host localhost --port 8081",
    "build": "parcel build --public-url . src/MapManager.ts",
    "lint": "eslint --fix --ext .ts src",
    "test": "jest"
  },
  "author": "Bingo-Soft",
  "license": "MIT",
  "dependencies": {
    "@turf/boolean-intersects": "^6.5.0",
    "@turf/turf": "^6.5.0",
    "@types/ol": "^6.4.2",
    "assets": "^3.0.1",
    "axios": "^0.18.1",
    "ol": "6.5.0",
    "ol-ext": "^3.1.17",
    "proj4": "^2.7.5",
    "src": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.0",
    "@types/proj4": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "jest": "^27.0.6",
    "parcel-bundler": "^1.12.4",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5"
  }
}
